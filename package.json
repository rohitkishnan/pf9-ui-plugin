  "scripts": {
    "build": "rm -rf ./build && cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 ./node_modules/.bin/webpack",
    "build-storybook": "build-storybook",
    "dev": "cross-env NODE_ENV=development yarn start",
    "lint": "./node_modules/.bin/eslint --ext .jsx,.js,.tsx,.ts src",
    "fix-lint": "./node_modules/.bin/eslint --ext .jsx,.js,.tsx,.ts --fix src",
    "start": "webpack-dev-server --progress --colors --hot --inline --env.development --host 0.0.0.0",
    "server": "./node_modules/.bin/babel-node --inspect --presets=@babel/preset-env src/server/main.js",
    "monitor": "./node_modules/.bin/nodemon --watch server/**/* --exec yarn server",
    "all": "concurrently --kill-others \"yarn monitor\" \"yarn start\"",
    "docker:build": " docker build -t ui-dev .",
    "docker:tag": " docker tag ui-dev platform9/ui-dev",
    "docker:push": " docker push platform9/ui-dev",
    "docker:run": "docker run -p 3000:3000 -p 4444:4444 ui-dev",
    "storybook": "start-storybook -s src/app/static -p 9001 -c src/stories",
    "test": "yarn lint && yarn test:unit",
    "test:client": "cross-env NODE_ENV=test jest src/api-client",
    "test:client:watch": "cross-env NODE_ENV=test jest --watch src/api-client",
    "test:e2e": "cross-env NODE_ENV=test jest -i e2e-tests/",
    "test:integration": "cross-env NODE_ENV=test cypress run",
    "test:integration:watch": "cross-env NODE_ENV=test cypress open",
    "cypress": "cross-env NODE_ENV=test cypress open",
    "cypress:record": "cross-env NODE_ENV=test cypress run --record --key $CYPRESS_RECORD_KEY",
    "test:unit": "cross-env NODE_ENV=test jest --coverage src/app src/server",
    "test:unit:watch": "cross-env NODE_ENV=test jest --watch --coverage src/app src/server"
  },
  "keywords": [
    "Platform9",
    "ui",
    "react",
    "plugin"
  ],
  "author": "Platform9, Inc.",
  "license": "Apache-2.0",
  "jest": {
    "automock": false,
    "setupFilesAfterEnv": [
      "./setupJest.js"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^developer(.*)$": "<rootDir>/src/app/plugins/developer$1",
      "^k8s(.*)$": "<rootDir>/src/app/plugins/kubernetes$1",
      "^openstack(.*)$": "<rootDir>/src/app/plugins/openstack$1",
      "^core(.*)$": "<rootDir>/src/app/core$1",
      "^utils(.*)$": "<rootDir>/src/app/utils$1",
      "^app(.*)$": "<rootDir>/src/app$1",
      "^server(.*)$": "<rootDir>/src/server$1",
      "^api-client(.*)$": "<rootDir>/src/api-client$1"
    }
  },
  "repository": "https://github.com/platform9/pf9-ui-plugin.git"
}
