  },
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "build-storybook": "build-storybook",
    "dev": "NODE_ENV=development npm start",
    "lint": "./node_modules/.bin/eslint src",
    "start": "./node_modules/.bin/babel-node src/devserver/server.js",
    "server": "./node_modules/.bin/babel-node src/server/main.js",
    "all": "concurrently --kill-others \"npm run server\" \"npm start\"",
    "docker:build": " docker build -t ui-dev .",
    "docker:tag": " docker tag ui-dev platform9/ui-dev",
    "docker:push": " docker push platform9/ui-dev",
    "docker:run": "docker run -p 3000:3000 -p 4444:4444 ui-dev",
    "storybook": "start-storybook -p 9001 -c src/stories",
    "test": "npm run lint && npm run test:unit",
    "test:client": "NODE_ENV=test jest src/api-client",
    "test:client:watch": "NODE_ENV=test jest --watch src/api-client",
    "test:e2e": "NODE_ENV=test jest -i e2e-tests/",
    "test:integration": "NODE_ENV=test jest integration-tests/",
    "test:unit": "NODE_ENV=test jest --coverage src/app src/server",
    "test:unit:watch": "NODE_ENV=test jest --watch --coverage src/app src/server"
  },
  "keywords": [
    "Platform9",
    "ui",
    "react",
    "plugin"
  ],
  "author": "Platform9, Inc.",
  "license": "ISC",
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "./setupJest.js",
    "testEnvironment": "node"
  }
}
